<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#3D3D42" />
            <corners android:radius="4dp" />
            <solid android:color="#313136" />
            <size
                android:width="16dp"
                android:height="16dp">
